{
    "version": 2,
    "builds": [
        {
            "src": "index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.py"
        }
    ],
    "env": {
        "PYTHONUNBUFFERED": "1"
    },
    "functions": {
        "api/*.py": {
            "maxDuration": 30
        }
    }
}
